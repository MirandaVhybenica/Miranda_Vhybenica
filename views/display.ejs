<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>USERS LIST</title>

   
</head>
<body>
    <table border="1">
        <tr>
            <th>First Name</th><br>
            <th>Last Name</th>
            <th>Gender</th>
            <th>Address</th>
            <th>Action</th>
        </tr>
        <% information.forEach(info => { %>
            <td><%= info.firstname %></td>
            <td><%= info.lastname %></td>
            <td><%= info.gender %></td>
            <td><%= info.address %></td>
            <td>
                <a href="/delete/<%= info.id%>">delete</a>
                <a href="/edit/<%= info.id%>">edit</a>
            </td>
        </tr>
        <% }) %>
    </table>
    <br><br>
    <a href="/">ADD USER</a>
    <br>
</body>
</html>